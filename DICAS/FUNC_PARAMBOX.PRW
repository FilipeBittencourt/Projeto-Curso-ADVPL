  Local cQryBkp   := ""
  Local cQry      := ""
  Local cSQLFSB1  := ""
  Local cSQLFAE8  := ""
  Local cSQLFAF9  := ""
  Local cCodTasks := ""
  Local cQryFilAFM:= ""
