  Local nPosTrf
  Local nDecCst	:= TamSX3("AF9_CUSTO")[2]
  Local cFilAF9	:= xFilial("AF9")

  Local nCusto	:= 0
  Local nFerram	:= 0
  Local nDesp		:= 0
  Local nSubComp	:= 0
  Local nCustoA	:= 0
  Local nCustoB	:= 0
  Local nCustoE	:= 0
  Local nCustoF	:= 0
  Local cTrunca	:= "2"
  Local nPerc
