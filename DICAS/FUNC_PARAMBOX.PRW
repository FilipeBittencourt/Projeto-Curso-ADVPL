  Do Case
  Case cAlias == "AF8"
    lAuth := PmsChkUser(AF8->AF8_PROJET, , ;
      Padr(AF8->AF8_PROJET, Len(AFC->AFC_EDT)), ;
      "  ", 1, "ESTRUT", AF8->AF8_REVISA)
  Case cAlias == "AFC"

    lAuth := PmsChkUser(AFC->AFC_PROJET, , AFC->AFC_EDT, ;
      AFC->AFC_EDTPAI, 1, "ESTRUT", AFC->AFC_REVISA)
  Case cAlias == "AF9"
