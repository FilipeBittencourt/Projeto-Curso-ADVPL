        SB1->(MsSeek(xFilial()+AF3->AF3_PRODUT))
        M->AF3_TIPO	:= SB1->B1_TIPO
        M->AF3_UM	:= SB1->B1_UM
        M->AF3_SEGUM := SB1->B1_SEGUM
        M->AF3_DESCRI:= SB1->B1_DESC
