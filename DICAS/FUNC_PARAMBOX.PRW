  Local oDlg ,oFont ,oBar, oGantt
  Local lLoop        := .T.
  Local lGrava	   := .F.
  Local lFWGetVersao := .T.

  Local aArea		   := GetArea()
  Local aGantt       := {}
  Local aTmp         := {}
  Local aButton	   := {}
  Local aCorBarras   := {}
