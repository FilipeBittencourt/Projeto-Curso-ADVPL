*/
Function PmsChkAJE(lAviso)
  Local lRet := .F.
  Local aArea	:= GetArea()

  dbSelectArea("SX3")
  dbSetOrder(2)
  If MsSeek("AJE_PROJET")
    lRet := .T.
  Else
    If lAviso
