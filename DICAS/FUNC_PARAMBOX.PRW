            EndCase

            If lViewRec
              PmsAddRsrG(@aGant, AF9->AF9_PROJET, AF9->AF9_REVISA, AF9->AF9_TAREFA, nContLin, SPACE((VAL(AF9->AF9_NIVEL)-1)*2), ValorCorBarra( "3" ,aCorBarras ))
