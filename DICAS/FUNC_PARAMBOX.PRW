      Case cAlias == "AJ2"
        aAdd(aSVAlias,"AJ2")
        oPanel:Hide()
        aEnch[8]:= MsMGet():New("AJ2",AJ2->(RecNo()),2,,,,,aPos,,3,,,,oPanel,,,lOneColumn)
        oPanel:Show()
        nOldEnch:=8
      Case cAlias == "AJ3"
        aAdd(aSVAlias,"AJ3")
        oPanel:Hide()
        aEnch[9]:= MsMGet():New("AJ3",AJ3->(RecNo()),2,,,,,aPos,,3,,,,oPanel,,,lOneColumn)
        oPanel:Show()
