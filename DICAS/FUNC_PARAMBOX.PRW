Static Function AddRange(aMV_PAR,cTipo,nSize)
  Local nUltimo := Len(aMV_PAR)
  Local lRet	:=	.F.
  If &(ReadVar()) == aMV_PAR[nUltimo,2]
    lRet	:=	.F.
  ElseIf !Empty(aMV_PAR[nUltimo,2])
    If cTipo == "C"
      AAdd(aMV_PAR,{Space(nSize),Space(nSize)})
    ElseIf cTipo == "D"
      AAdd(aMV_PAR,{ctod(''),ctod('')})
    Endif
    lRet	:=	.T.
  Endif
Return lRet


