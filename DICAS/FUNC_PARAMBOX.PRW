*/
Function PmsRetCRTE(aArrayTrb,nTipo,cCodigo)
  Local aCusto := {0,0,0,0,0,{}}

  Do Case
  Case nTipo == 1
    nPosSeek := aScan(aArrayTrb,{|x|x[1]==cCodigo})
    If nPosSeek>0
      aCusto := aArrayTrb[nPosSeek,3]
    EndIf
  Case nTipo == 2
    nPosSeek := aScan(aArrayTrb,{|x|x[2]==cCodigo})
    If nPosSeek>0
      aCusto := aArrayTrb[nPosSeek,3]
    EndIf
  EndCase

Return aCusto
