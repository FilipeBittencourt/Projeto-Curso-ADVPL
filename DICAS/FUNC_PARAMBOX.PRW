        aEnch[6]:Show()
        noldEnch:=6
      Case cAlias == "AJ1"
        aEnch[7]:EnchRefreshAll()
        aEnch[7]:Show()
        noldEnch:=7
      Case cAlias == "AJ2"
        aEnch[8]:EnchRefreshAll()
        aEnch[8]:Show()
        noldEnch:=8
      Case cAlias == "AJ3"
        aEnch[9]:EnchRefreshAll()
        aEnch[9]:Show()
        noldEnch:=9
