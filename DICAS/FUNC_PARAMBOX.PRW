  If ParamBox({	{3,STR0102,aConfig[1],{STR0103,STR0104,STR0105,STR0192,STR0193,STR0342},60,,.F.},; //"Escala de Tempo"###"Diario"###"Semanal"###"Mensal"###"Mensal (Zoom 30%)"###"Bimestral"
    {4,STR0106,aConfig[2],STR0341,70,,.F.},; //
