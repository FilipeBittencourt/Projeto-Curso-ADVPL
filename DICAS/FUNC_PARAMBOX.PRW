  Local dDtConv
  Local aTX2M		:= {0,0,0,0,0}
  Local lTopConn	:= IIf(Type("__lTopConn") == "L",__lTopConn,IfDefTopCTB())
  Local cAliasQry := ""
  Local cQuery	:= ""
  Local cFilAFA		:= xFilial("AFA")
  Local cFilAFB		:= xFilial("AFB")
