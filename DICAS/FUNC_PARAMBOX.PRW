Function RetNulos(cStr)
  Local cBuffer := ""
  Local nx := 0

  For nx := 1 To Len(cStr)
    If Substr(cStr,nx,1)==Chr(0)
      cBuffer += Chr(32)
    Else
      cBuffer += Substr(cStr, nx, 1)
    EndIf
  Next
Return cBuffer

