  Local nTarefa  := 0
  Local nTsk	   := 1
  Local nTop	   := oMainWnd:nTop+23
  Local nLeft	   := oMainWnd:nLeft+5
  Local nBottom  := oMainWnd:nBottom-60
  Local nRight   := oMainWnd:nRight-10

