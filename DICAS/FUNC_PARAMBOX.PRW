    ElseIf (cArquivo)->CTRLNIV == "+"
		aExpand[nPos,2] := .T.
    ElseIf (cArquivo)->CTRLNIV == "*"
		nNivelMax := 2000
		aExpand := {}
		dbSelectArea(cArquivo)
		dbGotop()
      If !InTransact()
			Zap
			Pack
      Else
        While !EOF()
				RecLock(cArquivo,.F.)
				DbDelete()
				MsUnLock()
				DbSkip()
        Enddo
      Endif
    ElseIf (cArquivo)->CTRLNIV == "!"
		nNivelMax := 1000
		aExpand := {}
    EndIf
  Else
    If (cArquivo)->CTRLNIV == "-"
		aAdd(aExpand,{(cArquivo)->ALIAS+(cArquivo)->XF2_TAREFA,.F.})
    ElseIf (cArquivo)->CTRLNIV == "+"
		aAdd(aExpand,{(cArquivo)->ALIAS+(cArquivo)->XF2_TAREFA,.T.})
    ElseIf (cArquivo)->CTRLNIV == "*"
		nNivelMax := 2000
		aExpand := {}
		dbSelectArea(cArquivo)
		dbGotop()
      If !InTransact()
			Zap
			Pack
      Else
        While !EOF()
				RecLock(cArquivo,.F.)
				DbDelete()
				MsUnLock()
				DbSkip()
        Enddo
      Endif
    ElseIf (cArquivo)->CTRLNIV == "!"
		nNivelMax := 1000
		aExpand := {}
    EndIf
  EndIf

Return

