Function PmsFilial(cTabOrig,cTabRelac,cMSFIL)

  Local nPosCpo	:= 0
  Local cRet		:= ''
  DEFAULT cMSFil	:= cTabRelac+"_MSFIL"

  nPosCpo := (cTabRelac)->(ColumnPos(cMSFil))

  If  nPosCpo > 0
    cRet := xFilial(cTabOrig,(cTabRelac)->(FieldGet(nPosCpo)))
  Else
    cRet :=  xFilial(cTabOrig)
  EndIf
Return cRet

