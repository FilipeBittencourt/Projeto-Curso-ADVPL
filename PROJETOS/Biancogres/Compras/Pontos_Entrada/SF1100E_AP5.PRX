		R0001 += "        DS_HORAPRE = ' '
		R0001 += "  WHERE DS_FILIAL = '"+xFilial("SDS")+"'
		R0001 += "    AND DS_CHAVENF = '"+SF1->F1_CHVNFE+"'
		R0001 += "    AND D_E_L_E_T_ = ' '
		TCSQLEXEC(R0001)
	EndIf

EndIf
*/
