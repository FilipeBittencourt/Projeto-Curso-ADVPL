// Processo de Devoluo. Includo por Marcos Alberto em 11/10/11 a pedido da Diretoria.
// Rotinas envolvidas: BIA267, SF1100I, SF1100E, SD1100I, MT100LOK, MT100GRV
If SF1->F1_TIPO == "D"
	A0002 := " SELECT Z26_NUMPRC, Z26_NFISC, Z26_SERIE, R_E_C_N_O_ REGZ26
	A0002 += "   FROM "+ RetSqlName("Z26")
	A0002 += "  WHERE Z26_FILIAL = '"+xFilial("Z26")+"'
	A0002 += "    AND Z26_OBS = '"+SF1->F1_DOC+SF1->F1_SERIE+"'
	A0002 += "    AND Z26_ITEMNF = 'XX'
	A0002 += "    AND D_E_L_E_T_ = ' '
	TcQuery A0002 New Alias "A002"
	dbSelectArea("A002")
	dbGoTop()
	While !Eof()
		xd_NumPrc := A002->Z26_NUMPRC
		xd_RegZ26 := A002->REGZ26
		
		dbSelectArea("Z26")
		dbGoTo(xd_RegZ26)
		RecLock("Z26",.F.)
		Z26->Z26_ITEMNF := " "
		Z26->Z26_OBS    := " "
		MsUnLock()
		
		// Retirado em 04/11/11 para atender a um padro que est sendo estabelecido para a Biancogres/Incesa, pois antes de lanar a nota no sistema  dado o parecer fis.
		// Foi necessrio retornar esta condio com alguns ajustes porque estava gerando duplicidade nos casos em que a nota era excluda.
		dbSelectArea("Z25")
		dbSetOrder(1)
		dbSeek(xFilial("Z25")+xd_NumPrc)
		dbSelectArea("Z26")
		dbGoTo(xd_RegZ26)
		While !Eof() .and. Z26->Z26_NUMPRC == xd_NumPrc
			If Z26->Z26_NFISC == A002->Z26_NFISC .and. Z26->Z26_SERIE == A002->Z26_SERIE .and. !Empty(Z26->Z26_ITEMNF)
				If Z25->Z25_APRFIS == " " .or. Z25->Z25_RETMRC == "N"
					dbSelectArea("Z26")
					RecLock("Z26",.F.)
					Delete
					MsUnLock()
				EndIf
			EndIf
			dbSelectArea("Z26")
			dbSkip()
		End
		
		dbSelectArea("Z25")
		dbSetOrder(1)
		If dbSeek(xFilial("Z25")+xd_NumPrc)
			RecLock("Z25",.F.)
			Z25->Z25_NFLANC := " "
			Z25->Z25_USLNFE := " "
			Z25->Z25_DTLNFE := ctod("  /  /  ")
			MsUnLock()
		EndIf
		
		dbSelectArea("A002")
		dbSkip()
	End
	A002->(dbCloseArea())
EndIf

