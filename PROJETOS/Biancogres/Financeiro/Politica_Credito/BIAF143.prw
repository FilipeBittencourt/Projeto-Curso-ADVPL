	cPath																	, ; //X2_PATH
	'ZM2010'																, ; //X2_ARQUIVO
	'Saldo Diario Cliente'														, ; //X2_NOME
	'Saldo Diario Cliente'														, ; //X2_NOMESPA
	'Saldo Diario Cliente'														, ; //X2_NOMEENG
	'C'																		, ; //X2_MODO
	''																		, ; //X2_TTS
	''																		, ; //X2_ROTINA
	''																		, ; //X2_PYME
	''																		, ; //X2_UNICO
	''																		, ; //X2_DISPLAY
	''																		, ; //X2_SYSOBJ
	''																		, ; //X2_USROBJ
	''																		, ; //X2_POSLGT
	''																		, ; //X2_CLOB
	''																		, ; //X2_AUTREC
	'C'																		, ; //X2_MODOEMP
	'C'																		, ; //X2_MODOUN
	0																		} ) //X2_MODULO

//
// Tabela ZM3
//
aAdd( aSX2, { ;
	'ZM3'																	, ; //X2_CHAVE
	cPath																	, ; //X2_PATH
	'ZM3010'																, ; //X2_ARQUIVO
	'Processos Rocket'											, ; //X2_NOME
	'Processos Rocket'											, ; //X2_NOMESPA
	'Processos Rocket'											, ; //X2_NOMEENG
	'C'																		, ; //X2_MODO
	''																		, ; //X2_TTS
	''																		, ; //X2_ROTINA
	''																		, ; //X2_PYME
	''																		, ; //X2_UNICO
	''																		, ; //X2_DISPLAY
	''																		, ; //X2_SYSOBJ
	''																		, ; //X2_USROBJ
	''																		, ; //X2_POSLGT
	''																		, ; //X2_CLOB
