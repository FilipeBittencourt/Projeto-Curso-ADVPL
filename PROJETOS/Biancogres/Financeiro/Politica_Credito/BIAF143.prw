             "X2_POSLGT" , "X2_CLOB"   , "X2_AUTREC" , "X2_MODOEMP", "X2_MODOUN" , "X2_MODULO" }


dbSelectArea( "SX2" )
SX2->( dbSetOrder( 1 ) )
SX2->( dbGoTop() )
cPath := SX2->X2_PATH
cPath := IIf( Right( AllTrim( cPath ), 1 ) <> "\", PadR( AllTrim( cPath ) + "\", Len( cPath ) ), cPath )
cEmpr := Substr( SX2->X2_ARQUIVO, 4 )

//
// Tabela ZM0
//
aAdd( aSX2, { ;
	'ZM0'																	, ; //X2_CHAVE
	cPath																	, ; //X2_PATH
	'ZM0010'																, ; //X2_ARQUIVO
	'Processos Politica Credito'											, ; //X2_NOME
	'Processos Politica Credito'											, ; //X2_NOMESPA
