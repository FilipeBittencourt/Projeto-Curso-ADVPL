//
// Tabela ZM4
//
aAdd( aSX2, { ;
	'ZM4'																	, ; //X2_CHAVE
	cPath																	, ; //X2_PATH
	'ZM4010'																, ; //X2_ARQUIVO
	'Retorno Prcessos Rocket'												, ; //X2_NOME
	'Retorno Prcessos Rocket'												, ; //X2_NOMESPA
	'Retorno Prcessos Rocket'												, ; //X2_NOMEENG
	'C'																		, ; //X2_MODO
	''																		, ; //X2_TTS
	''																		, ; //X2_ROTINA
	''																		, ; //X2_PYME
