            AND SA6.A6_FILIAL=%xFilial:SA6%
            AND SA6.A6_COD=%exp:cA6COD%
            AND SA6.A6_AGENCIA=%exp:cA6AGENCIA%
            AND SA6.A6_NUMCON=%exp:cA6NUMCON%
